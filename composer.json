{
    "name": "czende/balikonos-shipping-export-plugin",
    "type": "sylius-plugin",
    "description": "Balikonos shipping export plugin for Sylius based applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Czernin",
            "email": "jan.czernin@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/czende/ShippingExportPlugin"
        }
